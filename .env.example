APP_NAME=ProofgenRedux
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://proofgenredux.test

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Default AWS/S3 settings
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Sample Images S3 settings
SAMPLE_IMAGES_S3_KEY=
SAMPLE_IMAGES_S3_SECRET=
SAMPLE_IMAGES_S3_REGION=nyc3
SAMPLE_IMAGES_S3_BUCKET=your-sample-images-bucket
SAMPLE_IMAGES_S3_ENDPOINT=https://nyc3.digitaloceanspaces.com
SAMPLE_IMAGES_S3_PATH_STYLE=true
AUTO_DOWNLOAD_SAMPLE_IMAGES=false

# For Digital Ocean Spaces, use an endpoint like https://nyc3.digitaloceanspaces.com
# For AWS S3, leave endpoint blank
# Set AUTO_DOWNLOAD_SAMPLE_IMAGES=true to automatically download during tests

VITE_APP_NAME="${APP_NAME}"

# Proofgen Configuration
TEST_SOURCE_DIR=
FULLSIZE_HOME_DIR=
ARCHIVE_HOME_DIR=
ARCHIVE_ENABLED=FALSE

RENAME_FILES=TRUE
UPLOAD_PROOFS=TRUE

# Watermark Configuration
WATERMARK_PROOFS=TRUE
WATERMARK_FONT=
WATERMARK_BACKGROUND_OPACITY=70
WATERMARK_FOREGROUND_OPACITY=45

# Web Images Configuration
WEB_SUFFIX=_web
WEB_WIDTH=1600
WEB_HEIGHT=1600
WEB_QUALITY=100
WEB_FONT_SIZE=22
WEB_BG_SIZE=42

# High Resolution Images Configuration
HIGHRES_SUFFIX=_highres
HIGHRES_WIDTH=3000
HIGHRES_HEIGHT=3000
HIGHRES_QUALITY=95
HIGHRES_FONT_SIZE=22
HIGHRES_BG_SIZE=42

# Thumbnail Configuration
LARGE_THUMBNAIL_SUFFIX=_std
LARGE_THUMBNAIL_WIDTH=950
LARGE_THUMBNAIL_HEIGHT=950
LARGE_THUMBNAIL_QUALITY=30
LARGE_THUMBNAIL_FONT_SIZE=22
LARGE_THUMBNAIL_BG_SIZE=42

SMALL_THUMBNAIL_SUFFIX=_thm
SMALL_THUMBNAIL_WIDTH=250
SMALL_THUMBNAIL_HEIGHT=250
SMALL_THUMBNAIL_QUALITY=60
SMALL_THUMBNAIL_FONT_SIZE=12
SMALL_THUMBNAIL_BG_SIZE=26

# Processing Configuration
MAX_IMAGES_PER_RUN=500000
PHP_MEMORY_LIMIT=1024M
PHP_POOL_THREAD_COUNT=2
POOLED_THUMBNAIL_GENERATION=TRUE
THREAD_COUNT=6

# SFTP Configuration
SFTP_HOSTNAME=
SFTP_USERNAME=
SFTP_PATHTOPRIVATEKEY=
SFTP_PROOFSPATH=
SFTP_WEB_IMAGES_PATH=
SFTP_HIGHRES_IMAGES_PATH=
